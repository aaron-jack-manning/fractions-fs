<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <WarnOn>3390;$(WarnOn)</WarnOn>
    <Description>Implementation of arithmetic with fractions to allow exact calculations with non integers.</Description>
    <Company>$(Authors)</Company>
    <Authors>Aaron Manning</Authors>
    <RepositoryUrl>https://github.com/aaron-jack-manning/fractions-fs</RepositoryUrl>
    <Version>2.0.0</Version>
  </PropertyGroup>

  <ItemGroup>
    <Compile Include="Library.fs" />
  </ItemGroup>

</Project>
